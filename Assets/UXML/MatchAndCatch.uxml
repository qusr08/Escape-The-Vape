    <engine:VisualElement class="uofr-background">
        <engine:VisualElement name="GameScreen" class="uofr-screen" style="display: flex;">
            <engine:VisualElement class="uofr-vert-list uofr-section-small" style="justify-content: flex-end;">
                <engine:VisualElement class="uofr-hori-list uofr-vert-list-first">
                    <engine:Label text="Match &amp; Catch" class="uofr-text-large" style="flex-grow: 1;" />
                    <engine:Button name="PauseButton" class="uofr-button uofr-button-pause">
                        <engine:VisualElement style="background-image: url(&quot;project://database/Assets/Sprites/PauseIcon.png?fileID=2800000&amp;guid=bee2404f4685eed4baf6068a8203dffe&amp;type=3#PauseIcon&quot;); flex-grow: 1;" />
                    </engine:Button>
                </engine:VisualElement>
                <engine:Label text="Score: &lt;b&gt;320&lt;/b&gt;" name="ScoreLabel" class="uofr-text-small uofr-vert-list-last uofr-text-gray" />
            </engine:VisualElement>
            <engine:VisualElement style="flex-grow: 1;">
                <engine:VisualElement name="TutorialSubscreen" class="uofr-vert-list uofr-subscreen" style="align-items: center; opacity: 0; display: none;">
                    <engine:VisualElement class="uofr-vert-list-first" style="width: 65%; background-image: url(&quot;project://database/Assets/Sprites/FingerIcon.png?fileID=2800000&amp;guid=87bb5bd2daa6a2546a91f1b9fa8891f3&amp;type=3#FingerIcon&quot;); height: 0; padding-top: 65%;" />
