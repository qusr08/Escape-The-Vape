                </engine:VisualElement>
                <engine:VisualElement name="GameSubscreen" class="uofr-vert-list uofr-subscreen" style="align-items: center; display: flex; opacity: 1;">
                    <engine:Label text="6 matches left!" name="MatchesLabel" class="uofr-text-medium uofr-section-small" style="-unity-text-align: middle-center;" />
                    <engine:VisualElement class="uofr-hori-list mandc-card-row uofr-vert-list-first">
