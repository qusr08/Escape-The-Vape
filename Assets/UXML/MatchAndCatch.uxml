            </engine:VisualElement>
            <engine:VisualElement class="uofr-vert-list uofr-section-medium">
                <engine:Label text="Total Score" class="uofr-text-medium uofr-vert-list-first" style="-unity-text-align: middle-center;" />
                <engine:Label text="600 points" name="TotalScoreLabel" class="uofr-vert-list-last uofr-text-large" style="-unity-text-align: middle-center;" />
            </engine:VisualElement>
            <engine:VisualElement class="uofr-vert-list">
                <engine:Button text="Play Again" name="PlayAgainButton" class="uofr-button uofr-button-selected uofr-vert-list-first" />
                <engine:Button text="Home" name="HomeButton" class="uofr-button uofr-vert-list-last" />
            </engine:VisualElement>
        </engine:VisualElement>
        <engine:VisualElement name="PauseScreen" class="uofr-screen uofr-vert-list" style="display: none;">
            <engine:Label text="Paused" class="uofr-text-large uofr-vert-list-first uofr-section-small" style="-unity-text-align: middle-center;" />
            <engine:Button text="Resume" name="ResumeButton" class="uofr-button uofr-button-selected uofr-vert-list-middle" />
            <engine:Button text="Quit" name="QuitButton" class="uofr-button uofr-vert-list-last" />
