                    </engine:VisualElement>
                </engine:VisualElement>
            </engine:VisualElement>
        </engine:VisualElement>
        <engine:VisualElement name="WinScreen" class="uofr-screen" style="display: none;">
            <engine:VisualElement class="uofr-vert-list uofr-section-small">
                <engine:Label text="Final Score" class="uofr-text-medium uofr-vert-list-first" style="-unity-text-align: middle-center;" />
